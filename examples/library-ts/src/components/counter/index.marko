export interface Input {
  multiplier?: number;
}
class {
  declare state: {
    count: number;
  };

  onCreate() {
    this.state = {
      count: 0,
    };
  }

  increment() {
    this.state.count++;
    this.emit("increment");
  }
}

<button onClick("increment")>
  ${state.count * (input.multiplier ?? 1)}
</button>
