{
  "name": "lasso-koa-example",
  "description": "Sample Koa-based server app that integrates with Marko.",
  "version": "1.0.0",
  "dependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/register": "^7.13.0",
    "@lasso/marko-taglib": "^2.0.3",
    "browser-refresh": "^1.7.3",
    "browser-refresh-taglib": "^1.1.0",
    "koa": "^2.13.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "lasso": "^3.4.1",
    "lasso-marko": "^3.0.1",
    "marko": "^5.1.20"
  },
  "private": true,
  "scripts": {
    "dev": "browser-refresh --enable-source-maps -r @babel/register -r marko/node-require src/server.js",
    "start": "NODE_ENV=production node -r @babel/register -r marko/node-require src/server.js"
  }
}
